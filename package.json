{
  "name": "rel-parser",
  "version": "1.1.0",
  "description": "A simple function to scrape rel links from a url or html document",
  "type": "module",
  "main": "dist/main.cjs",
  "module": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    ".": {
      "require": "./dist/main.js",
      "import": "./dist/main.mjs",
      "types": "./dist/main.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "fix": "run-s fix:lint",
    "fix:lint": "ts-standard --fix src",
    "test": "run-s test:lint test:build test:unit",
    "test:build": "tsup --env.TEST=yes",
    "test:lint": "ts-standard src",
    "test:unit": "ava"
  },
  "license": "MIT",
  "homepage": "https://github.com/grantcodes/start-ts-module#readme",
  "engines": {
    "node": ">=14"
  },
  "bugs": {
    "url": "https://github.com/grantcodes/rel-parser",
    "email": "mail@grant.codes"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/grantcodes/rel-parser.git"
  },
  "author": {
    "url": "https://grant.codes",
    "name": "Grant Richmond",
    "email": "mail@grant.codes"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "jsdom": "^22.1.0",
    "li": "^1.3.0"
  },
  "browser": {
    "jsdom/lib/jsdom/living": false
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@grantcodes/commit": "^1.0.7",
    "ava": "^5.3.1",
    "npm-run-all": "^4.1.5",
    "ts-standard": "^12.0.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
